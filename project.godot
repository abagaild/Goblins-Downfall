; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Goblin's Downfall"
run/main_scene="res://scenes/splash_screen.tscn"
config/features=PackedStringArray("4.4")
config/icon="res://icon.png"

[autoload]

EasyMenuManager="*res://addons/easy_menus/core/menu_manager.gd"
EasySaveLoadManager="*res://addons/easy_menus/core/save_load_manager.gd"
MenuManager="*res://addons/easy_menus/scripts/autoloads/MenuManager.gd"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Goblin-s Downfall"

[editor_plugins]

enabled=PackedStringArray("res://addons/easy_menus/plugin.cfg", "res://addons/scribbler/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
